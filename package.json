  "license": "MIT",
  "scripts": {
    "ng": "nx",
    "postinstall": "node ./decorate-angular-cli.js && ngcc --properties es2015 browser module main",
    "start": "nx serve storybook-server",
    "heroku-start": "node dist/apps/storybook-server/main.js",
    "build": "concurrently \"yarn run build-projects\" \"yarn run build-storybook\"",
    "build-projects": "nx run-many --target=build --projects=petstore,storybook-server",
    "build-storybook": "nx run ui:build-storybook",
    "test": "nx test"
  },
